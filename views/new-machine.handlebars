<div class="page-header">
  <h3></h3>
</div>
<div class="newMachine">
  <h4>New Instrument</h4>
  <form class="formNewMachine" role="form" action="process?form=formNewMachine" method="POST">

    <div class="form-group">
      <label for="fieldName" class="col-sm-2 control-label">Instrument Name</label>
      <div class="col-sm-4">
        <input type="text" class="form-control" id="fieldName" name="name" required>
      </div>
    </div><!-- End of form-group -->

    <div class="form-group">
      <label for="fieldRootNode" class="col-sm-2 control-label">First Instruction</label>
      <div class="col-sm-4">
        <input type="text" class="form-control" id="fieldRootNode" name="rootNode" required>
      </div>
    </div><!-- End of form-group -->

    <div class="form-group">
      <div class="col-sm-4">
        <button type="submit" class="btn btn-default">Submit</button>
      </div>
    </div><!-- End of form-group -->
  </form>
</div><!-- End if newMachine -->
